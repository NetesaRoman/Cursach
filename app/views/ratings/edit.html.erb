

<%= form_with id: 'modelForm', model: @rating do |form| %>
  <%= form.label :ip %>
  <%= form.text_field :ip, class: 'form-control' %>
  <%= form.label :star_id %>
  <%= form.text_field :star_id, class: 'form-control' %>
  <%= form.label :movie_id %>
  <%= form.text_field :movie_id, class: 'form-control' %>

   <%= form.submit %>
<% end %>
