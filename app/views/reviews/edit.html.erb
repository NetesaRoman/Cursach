

<%= form_with id: 'modelForm', model: @review do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, class: 'form-control' %>
  <%= form.label :email %>
  <%= form.text_field :email, class: 'form-control' %>
  <%= form.label :text %>
  <%= form.text_field :text, class: 'form-control' %>
  <%= form.label :parent_id %>
  <%= form.text_field :parent_id, class: 'form-control' %>
  <%= form.label :movie_id %>
  <%= form.text_field :movie_id, class: 'form-control' %>

   <%= form.submit %>
<% end %>
